<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#c1d7e9"
    tools:context="com.creativeerror.finalproject.MenuPilihan">

    <ImageButton
        android:id="@+id/QuizButton"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:srcCompat="@mipmap/quizicon"
        tools:layout_editor_absoluteX="48dp"
        tools:layout_editor_absoluteY="190dp"
        android:layout_marginEnd="44dp"
        android:layout_below="@+id/textView2"
        android:layout_toStartOf="@+id/BelajarButton2" />
    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Start quiz"
        android:fontFamily="casual"
        android:textColor="#000"
        android:id="@+id/textView"
        android:textSize="23dp"
        android:layout_above="@+id/QuizButton"
        android:layout_alignEnd="@+id/QuizButton" />
    <ImageButton
        android:id="@+id/BelajarButton2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:srcCompat="@mipmap/materi"
        android:layout_marginEnd="69dp"
        android:layout_centerVertical="true"
        android:layout_alignParentEnd="true" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Quiz Material"
        android:fontFamily="casual"
        android:textColor="#000"
        android:id="@+id/textView2"
        android:textSize="23dp"
        android:layout_above="@+id/BelajarButton2"
        android:layout_alignStart="@+id/BelajarButton2" />
</RelativeLayout>
